  "bugs": {
    "url": "https://github.com/sketchish/faceoff-be/issues"
  },
  "homepage": "https://github.com/sketchish/faceoff-be#readme",
  "dependencies": {
    "-": "^0.0.1",
    "@aws-sdk/client-s3": "^3.485.0",
    "@aws-sdk/lib-storage": "^3.485.0",
    "@aws-sdk/s3-request-presigner": "^3.485.0",
    "@types/lodash": "^4.14.202",
    "aws-sdk": "^2.1609.0",
    "bcrypt": "^5.1.1",
    "cors": "^2.8.5",
    "csv-parser": "^3.0.0",
    "D": "^1.0.0",
    "dotenv": "^16.3.1",
    "ejs": "^3.1.10",
    "email-templates": "^11.1.1",
    "express": "^4.18.2",
    "express-jwt": "^8.4.1",
    "fcm-node": "^1.6.1",
    "fs": "^0.0.1-security",
    "gts": "^5.2.0",
    "handlebars": "^4.7.8",
    "joi": "^17.11.0",
    "jsonwebtoken": "^9.0.2",
    "lodash": "^4.17.21",
    "moment": "^2.29.4",
    "mongoose": "^8.0.3",
    "multer": "^1.4.5-lts.1",
    "multer-s3": "^3.0.1",
    "nodemailer": "^6.9.7",
    "path": "^0.12.7",
    "winston": "^3.11.0",
    "winston-mongodb": "^5.1.1"
  },
